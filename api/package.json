  "scripts": {
    "cleanup": "docker volume prune -f && docker network prune -f && docker stop $(docker ps -a -q) || true && docker rm $(docker ps -a -q) || true",
    "up": "docker-compose up",
    "up:clean": "yarn run cleanup && docker-compose up",
    "up:clean:build": "yarn run cleanup && docker-compose up --build",
    "exec": "docker-compose exec api yarn run migrate && docker-compose exec api yarn run seed && docker exec -it $(docker ps -f name=api_1 -q) /bin/bash",
    "knex:install": "yarn global add knex@0.21.12",
    "start": "ts-node-dev --respawn ./src/app.ts",
    "build": "npx tsc -p .",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "eslint": "eslint \"**/*.ts\" --ignore-pattern node_modules/"
  },
