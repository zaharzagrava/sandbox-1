{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cleanup": "docker volume prune -f && docker network prune -f && docker stop $(docker ps -a -q) || true && docker rm $(docker ps -a -q) || true",
    "up": "docker-compose up",
    "up:clean": "yarn run cleanup && docker-compose up",
    "up:clean:build": "yarn run cleanup && docker-compose up --build",
    "exec": "docker exec -it $(docker ps -f name=api_1 -q) /bin/bash",
    "docker-start": "node ./src/server.js",
    "start": "node ./build/src/server.js",
    "dev": "ts-node-dev --respawn ./src/server.ts",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "build": "npx tsc -p ."
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.32",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/joi": "^14.3.4",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/knex": "^0.16.1",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.11.7",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "pg": "^8.4.0",
    "pg-hstore": "^2.3.3",
